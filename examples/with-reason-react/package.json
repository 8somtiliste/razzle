{
  "name": "razzle-examples-with-reason-react",
  "version": "4.0.0-canary.8",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run js-watch\" \"npm run bsb-watch\"",
    "js-watch": "razzle start",
    "bsb-watch": "bsb -make-world -w",
    "clean": "bsb -clean-world && rm -rf build",
    "build": "bsb -make-world && razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reason-react": "^0.8.0"
  },
  "devDependencies": {
    "bs-platform": "^7.2.2",
    "concurrently": "^5.2.0",
    "razzle": "4.0.0-canary.8",
    "razzle-dev-utils": "4.0.0-canary.8",
    "mini-css-extract-plugin": "^0.9.0",
    "webpack": "^4.44.1",
    "babel-preset-razzle": "4.0.0-canary.8",
    "webpack-dev-server": "^3.11.0"
  }
}